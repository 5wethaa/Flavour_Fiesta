<Project Sdk="Microsoft.NET.Sdk.Web">

	<PropertyGroup>
		<TargetFramework>net9.0</TargetFramework>
		<Nullable>disable</Nullable>
		<ImplicitUsings>enable</ImplicitUsings>
		<RootNamespace>Flavour_Fiesta</RootNamespace>
		<UserSecretsId>7d01e3ce-f446-47e8-bdad-670ca2ae8adf</UserSecretsId>
		<OutputType>WinExe</OutputType>
		
	</PropertyGroup>

	<ItemGroup>
		<Folder Include="wwwroot\images\" />
	</ItemGroup>

	<ItemGroup>
	  <None Include="wwwroot\css\_Layout.css" />
	</ItemGroup>

	<ItemGroup>
	  <PackageReference Include="Microsoft.EntityFrameworkCore" Version="9.0.6" />
	  <PackageReference Include="Microsoft.EntityFrameworkCore.Design" Version="9.0.6">
	    <PrivateAssets>all</PrivateAssets>
	    <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
	  </PackageReference>
	</ItemGroup>

	<ItemGroup>
		<ProjectReference Include="..\Flavour_Fiesta.Domain\Flavour_Fiesta.Domain.csproj" />
		<ProjectReference Include="..\Flavour_Fiesta.Service\Flavour_Fiesta.Service.csproj" />
	</ItemGroup>

</Project>
